{
  "manifest_version": 2,
  "name" : "__MSG_title__",
  "version": "0.1.6",
  "description": "__MSG_description__",
  "default_locale": "en",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/suite_16.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "icons/suite_128.png",
    "48": "icons/suite_48.png",
    "19": "icons/suite_19.png",
    "16": "icons/suite_16.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+D",
        "mac": "Alt+D",
        "chromeos": "Alt+D",
        "linux": "Alt+D"
      }
    }
  }
}
